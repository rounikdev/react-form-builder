{
  "all": true,
  "check-coverage": true,
  "exclude": ["**/*.cy.*", "**/*.test.*", "**/stories/**"],
  "extension": [".ts", ".tsx"],
  "include": ["src"],
  "instrument": true,
  "report-dir": "nyc/coverage",
  "reporter": ["clover", "cobertura", "json", "json-summary", "lcov", "text"],
  "temp-dir": "nyc/temp",
  "branches": 70,
  "lines": 80,
  "functions": 70,
  "statements": 80
}
